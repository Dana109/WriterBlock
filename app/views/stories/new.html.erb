<h1>Create a new story </h1>
<%= form_for @story do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %><br /><br />
	<%= f.label :genre %>
	<%= f.text_field :genre %><br /><br />
	<%= f.label :summary %>
	<%= f.text_field :summary %><br /><br />
	<%= f.label :content %>
	<%= f.text_field :content %><br /><br />
	<%= f.label :user %>
	<%= f.text_field :user %><br /><br />
	<%= f.submit 'Save'%>
<% end %>